{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "User list",
  "type": "array",
  "required": [
    "id"
  ],
  "properties": {
    "id": {
      "type": "number"
    },
    "screen_name": {
      "type": ["string", "null"]
    },
    "name": {
      "type": ["string", "null"]
    },
    "created_at": {
      "type": ["string", "null"],
      "description": "compliant with RFC3339"
    },
    "email": {
      "type": ["string", "null"]
    }
  }
}
